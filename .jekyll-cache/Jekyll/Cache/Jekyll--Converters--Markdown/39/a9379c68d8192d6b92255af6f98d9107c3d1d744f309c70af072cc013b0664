    <span class="k">print</span><span class="p">(</span><span class="n">tmp</span><span class="p">)</span>

<span class="k">for</span> <span class="p">(</span><span class="n">idx</span><span class="p">,</span> <span class="n">tmp</span><span class="p">)</span> <span class="ow">in</span> <span class="nb">enumerate</span><span class="p">(</span><span class="n">arr</span><span class="p">):</span>
    <span class="k">print</span><span class="p">(</span><span class="s">'ì¸ë±ìŠ¤ : {}, ë°ì´í„° : {}'</span><span class="p">.</span><span class="nb">format</span><span class="p">(</span><span class="n">idx</span><span class="p">,</span> <span class="n">tmp</span><span class="p">))</span>
<span class="s">'''
ì¸ë±ìŠ¤ : 0, ë°ì´í„° : 10
ì¸ë±ìŠ¤ : 1, ë°ì´í„° : 11
ì¸ë±ìŠ¤ : 2, ë°ì´í„° : 12
ì¸ë±ìŠ¤ : 3, ë°ì´í„° : 13
ì¸ë±ìŠ¤ : 4, ë°ì´í„° : 14
ì¸ë±ìŠ¤ : 5, ë°ì´í„° : 15
ì¸ë±ìŠ¤ : 6, ë°ì´í„° : 16
ì¸ë±ìŠ¤ : 7, ë°ì´í„° : 17
ì¸ë±ìŠ¤ : 8, ë°ì´í„° : 18
ì¸ë±ìŠ¤ : 9, ë°ì´í„° : 19
